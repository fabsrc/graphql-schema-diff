{
  "name": "graphql-schema-diff",
  "version": "2.7.0",
  "description": "Returns the diff of two GraphQL schemas. Detects dangerous and breaking changes.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --files src/cli",
    "prepack": "npm run build",
    "test": "jest",
    "fmt": "prettier --write src/**"
  },
  "bin": "dist/cli.js",
  "files": [
    "dist/",
    "npm-shrinkwrap.json"
  ],
  "keywords": [
    "graphql",
    "schema",
    "diff"
  ],
  "author": "Fabian Schneider",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabsrc/graphql-schema-diff/issues"
  },
  "homepage": "https://github.com/fabsrc/graphql-schema-diff#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabsrc/graphql-schema-diff"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/json-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
    "@graphql-tools/url-loader": "^7.17.18",
    "@graphql-tools/utils": "^10.0.0",
    "@highlightjs/cdn-assets": "^11.8.0",
    "chalk": "^4.1.2",
    "diff2html": "^3.4.35",
    "disparity": "^3.2.0",
    "fs-extra": "^11.1.1",
    "graphql": "^15.8.0",
    "meow": "^9.0.0",
    "node-fetch": "^2.6.11"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/hogan.js": "^3.0.1",
    "@types/jest": "^28.1.8",
    "@types/node": "^20.2.5",
    "@types/node-fetch": "^2.6.4",
    "jest": "^28.1.3",
    "nock": "^13.3.1",
    "prettier": "^2.8.8",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">= 12"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true
  }
