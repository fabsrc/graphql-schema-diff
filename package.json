{
  "name": "graphql-schema-diff",
  "version": "2.2.0",
  "description": "Returns the diff of two GraphQL schemas. Detects dangerous and breaking changes.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --files src/cli",
    "prepack": "npm run build",
    "test": "jest",
    "fmt": "prettier --write src/**"
  },
  "bin": "dist/cli.js",
  "files": [
    "dist/",
    "npm-shrinkwrap.json"
  ],
  "keywords": [
    "graphql",
    "schema",
    "diff"
  ],
  "author": "Fabian Schneider",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabsrc/graphql-schema-diff/issues"
  },
  "homepage": "https://github.com/fabsrc/graphql-schema-diff#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabsrc/graphql-schema-diff"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.0.1",
    "@graphql-tools/json-file-loader": "^7.0.1",
    "@graphql-tools/load": "^7.0.1",
    "@graphql-tools/url-loader": "^7.0.3",
    "chalk": "^4.1.2",
    "diff2html": "^3.4.8",
    "disparity": "^3.2.0",
    "fs-extra": "^10.0.0",
    "graphql": "^15.5.1",
    "meow": "^9.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/hogan.js": "^3.0.1",
    "@types/jest": "^26.0.24",
    "@types/meow": "^5.0.0",
    "@types/node": "^16.4.8",
    "@types/ws": "^7.4.7",
    "jest": "^27.0.6",
    "nock": "^13.1.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">= 12"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true
  }
}
