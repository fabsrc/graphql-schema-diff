{
  "name": "graphql-schema-diff",
  "version": "2.1.0",
  "description": "Returns the diff of two GraphQL schemas. Detects dangerous and breaking changes.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --files src/cli",
    "prepack": "npm run build",
    "test": "jest",
    "fmt": "prettier --write src/**"
  },
  "bin": "dist/cli.js",
  "files": [
    "dist/",
    "npm-shrinkwrap.json"
  ],
  "keywords": [
    "graphql",
    "schema",
    "diff"
  ],
  "author": "Fabian Schneider",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabsrc/graphql-schema-diff/issues"
  },
  "homepage": "https://github.com/fabsrc/graphql-schema-diff#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabsrc/graphql-schema-diff"
  },
  "dependencies": {
    "@graphql-toolkit/core": "^0.10.7",
    "@graphql-toolkit/graphql-file-loader": "^0.10.7",
    "@graphql-toolkit/json-file-loader": "^0.10.7",
    "@graphql-toolkit/url-loader": "^0.10.7",
    "chalk": "^4.1.0",
    "diff2html": "^3.1.13",
    "disparity": "^3.1.0",
    "fs-extra": "^9.0.1",
    "graphql": "^15.3.0",
    "meow": "^7.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/hogan.js": "^3.0.0",
    "@types/jest": "^26.0.14",
    "@types/meow": "^5.0.0",
    "@types/node": "^14.11.2",
    "jest": "^26.4.2",
    "nock": "^13.0.4",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true
  }
}
