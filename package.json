{
  "name": "graphql-schema-diff",
  "version": "2.6.1",
  "description": "Returns the diff of two GraphQL schemas. Detects dangerous and breaking changes.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --files src/cli",
    "prepack": "npm run build",
    "test": "jest",
    "fmt": "prettier --write src/**"
  },
  "bin": "dist/cli.js",
  "files": [
    "dist/",
    "npm-shrinkwrap.json"
  ],
  "keywords": [
    "graphql",
    "schema",
    "diff"
  ],
  "author": "Fabian Schneider",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabsrc/graphql-schema-diff/issues"
  },
  "homepage": "https://github.com/fabsrc/graphql-schema-diff#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabsrc/graphql-schema-diff"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.2",
    "@graphql-tools/json-file-loader": "^8.0.0",
    "@graphql-tools/load": "^7.7.4",
    "@graphql-tools/url-loader": "^7.13.9",
    "@graphql-tools/utils": "^9.0.1",
    "@highlightjs/cdn-assets": "^11.6.0",
    "chalk": "^4.1.2",
    "diff2html": "^3.4.18",
    "disparity": "^3.2.0",
    "fs-extra": "^10.1.0",
    "graphql": "^15.6.1",
    "meow": "^9.0.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/hogan.js": "^3.0.1",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.9",
    "@types/node-fetch": "^2.6.2",
    "jest": "^28.1.3",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">= 12"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true
  }
}
